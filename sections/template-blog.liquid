{% paginate blog.articles by 12 %}
    {% for article in blog.articles %}
        <div></div>
            {% if article.image != blank %}
                <img src="{{ article.image |img_url: 'medium' }}" alt="{{ article.image.alt }}" >
            {% else %}
                {{ 'image' | placeholder_svg_tag: 'w-20' }}
            {% endif %}
            <h1>{{ article.title }}</h1>
        </div>
    {% endfor %}
{% endpaginate %}